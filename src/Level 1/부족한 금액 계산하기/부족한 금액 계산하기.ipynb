    "def solution(price, money, count):\n",
    "    total = 0\n",
    "    \n",
    "    for i in range(count):\n",
    "        total += price * (i+1)\n",
    "    \n",
    "    if money >= total :\n",
    "        return 0\n",
    "    else:\n",
    "        return total - money "
